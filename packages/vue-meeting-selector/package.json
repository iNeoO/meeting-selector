{
  "name": "vue-meeting-selector",
  "version": "4.0.0",
  "private": false,
  "type": "module",
  "main": "dist/vue-meeting-selector.umd.js",
  "module": "dist/vue-meeting-selector.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-meeting-selector.es.js",
      "require": "./dist/vue-meeting-selector.umd.js"
    },
    "./style.css": {
      "default": "./dist/main.css"
    }
  },
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "dev": "run-s build dev:watch",
    "dev:watch": "chokidar 'src/**/*' -c 'pnpm build'",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "common-meeting-selector": "workspace:^",
    "vue": "catalog:"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.32",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/eslint-plugin": "^1.2.7",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "chokidar-cli": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-oxlint": "~1.1.0",
    "eslint-plugin-vue": "~10.2.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "catalog:",
    "oxlint": "catalog:",
    "prettier": "catalog:",
    "sass-embedded": "^1.89.2",
    "typescript": "catalog:",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "catalog:",
    "vue-tsc": "catalog:"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
